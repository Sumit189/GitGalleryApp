{
  "cli": {
    "version": ">= 16.23.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_GITHUB_CLIENT_ID": "xxxxxxxxxxx"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_GITHUB_CLIENT_ID": "xxxxxxxxxxx"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_GITHUB_CLIENT_ID": "xxxxxxxxxxx"
      }
    },
    "production-apk": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_GITHUB_CLIENT_ID": "xxxxxxxxxxx"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
